# Stock-A-Future 环境变量配置

# 数据源配置 - 使用AKTools
DATA_SOURCE_TYPE=aktools

# AKTools配置 - 运行在本地8080端口
AKTOOLS_BASE_URL=http://127.0.0.1:8080

# 服务器配置 - 使用不同的端口避免冲突
SERVER_PORT=8081
SERVER_HOST=localhost

# 日志级别
LOG_LEVEL=info

# 缓存配置
CACHE_ENABLED=true
CACHE_DEFAULT_TTL=1h
CACHE_MAX_AGE=24h
CACHE_CLEANUP_INTERVAL=10m

# 模式预测配置
PATTERN_PREDICTION_DAYS=14

# 数据窗口配置 - 默认拉取90天数据确保所有技术指标都有足够数据
DEFAULT_DATA_WINDOW_DAYS=90
