# Tushare API配置
TUSHARE_TOKEN=your_tushare_token_here
TUSHARE_BASE_URL=http://api.tushare.pro

# 服务器配置
SERVER_HOST=localhost
SERVER_PORT=8080

# 日志配置
LOG_LEVEL=info

# 缓存配置
# 是否启用缓存（默认：true）
CACHE_ENABLED=true

# 默认缓存过期时间（默认：1h）
# 支持的时间单位：s(秒), m(分钟), h(小时)
# 示例：30s, 10m, 2h, 1h30m
CACHE_DEFAULT_TTL=1h

# 最大缓存时间（默认：24h）
# 即使设置了更长的TTL，也不会超过这个时间
CACHE_MAX_AGE=24h

# 清理过期缓存的间隔（默认：10m）
# 后台会定期清理过期的缓存条目
CACHE_CLEANUP_INTERVAL=10m
